@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        <th>Quantity</th>
        <th>Price</th>



    </tr>
    @for (int i = 0; i < ViewBag.Items.Count; i++)
    {
        <tr>
            <td>@ViewBag.Items[i].ID</td>
            <td>@ViewBag.Items[i].Name</td>
            <td>@ViewBag.Items[i].Description</td>
            <td>@ViewBag.Items[i].Quantity</td>
            <td>@ViewBag.Items[i].Price</td>

        </tr>
    }

</table>

<a href="~/Home/Index" class="btn btn-default">Add another item.</a>
